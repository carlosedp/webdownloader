h2 Register

form(method='post', action='/user')
  div
    label Email:
    input(name='user[email]', value=user.email || '')
  div
    label Password:
    input(name='user[password]', value=user.password || '', type='password')
  div
    input(type='submit', value='Register').submit_button
    span \ or 
      a(href='/session/new') Log In


